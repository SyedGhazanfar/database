{
    "name": "orchestra-update/database",
    "description": "Database Component for Orchestra Platform",
    "keywords": ["orchestra-platform", "orchestral", "database"],
    "license": "MIT",
    "authors": [
        {
            "name": "Syed Ghazanfar",
            "email": "hafizghazanfar499@gmail.com",
            "homepage": "https://github.com/SyedGhazanfar/database"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Database\\": ""
        }
    },
    "require": {
         "php": "^7.4 || ^8.0 || ^8.1",
        "illuminate/contracts": "10.x-dev",
        "illuminate/database": "10.x-dev",
        "illuminate/support": "10.x-dev",
        "laravie-update/query-filter": "3.x-dev"
    },
    "suggest": {
        "laravie/query-filter": "Allow to use whereLike query builder macros with Laravel (dev-master)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "dev-master"
        },
        "laravel": {
            "providers": [
                "Orchestra\\Database\\CachableQueryServiceProvider",
                "Orchestra\\Database\\SearchServiceProvider"
            ]
        }
    },
    "support": {
        "issues": "https://github.com/SyedGhazanfar/database/pulls"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
